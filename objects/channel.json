{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "properties": {
    "id": {
      "type": "string",
      "id": "id"
    },
    "name": {
      "type": "string",
      "id": "name"
    },
    "is_channel": {
      "type": "boolean",
      "id": "is_channel"
    },
    "created": {
      "type": "integer",
      "id": "created"
    },
    "creator": {
      "type": "string",
      "id": "creator"
    },
    "is_archived": {
      "type": "boolean",
      "id": "is_archived"
    },
    "is_general": {
      "type": "boolean",
      "id": "is_general"
    },
    "members": {
      "type": "array",
      "items": {
        "type": "string"
      },
      "id": "members"
    },
    "topic": {
      "type": "object",
      "properties": {
        "value": {
          "type": "string",
          "id": "value"
        },
        "creator": {
          "type": "string",
          "id": "creator"
        },
        "last_set": {
          "type": "integer",
          "id": "last_set"
        }
      },
      "id": "topic"
    },
    "purpose": {
      "type": "object",
      "properties": {
        "value": {
          "type": "string",
          "id": "value"
        },
        "creator": {
          "type": "string",
          "id": "creator"
        },
        "last_set": {
          "type": "integer",
          "id": "last_set"
        }
      },
      "id": "purpose"
    },
    "is_member": {
      "type": "boolean",
      "id": "is_member"
    },
    "last_read": {
      "type": "string",
      "id": "last_read"
    },
    "latest": {
      "$ref": "message.json"
    },
    "unread_count": {
      "type": "integer",
      "id": "unread_count"
    },
    "unread_count_display": {
      "type": "integer",
      "id": "unread_count_display"
    },
    "name_normalized": {
      "type": "string"
    },
    "is_org_shared": {
      "type": "boolean"
    },
    "priority": {
      "type": "number"
    },
    "is_moved": {
      "type": "integer"
    },
    "accepted_user": {
      "type": "string"
    },
    "is_pending_ext_shared": {
      "type": "boolean"
    },
    "is_mpim": {
      "type": "boolean"
    },
    "is_read_only": {
      "type": "boolean"
    },
    "is_private": {
      "type": "boolean"
    },
    "previous_names": {
      "type": "array",
      "items": {
        "type": "string"
      }
    },
    "num_members": {
      "type": "integer"
    },
    "is_shared": {
      "type": "boolean"
    },
      "unread_count": {
      "type": "integer"
    },
    "unlinked": {
      "type": "integer",
      "description": "Field to determine whether a channel has ever been shared/disconnected in the past"
    }
  },
  "id": "channel"
}
